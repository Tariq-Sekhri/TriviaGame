<Window x:Class="TriviaGame.UserInfoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TriviaGame"
        mc:Ignorable="d"
        Title="UserInfoWindow" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="txtScore" HorizontalAlignment="Center" Margin="0,109,0,0" TextWrapping="Wrap" Text="" TextAlignment="Center" VerticalAlignment="Top" Width="509"/>
        <TextBlock x:Name="txtQuestions" HorizontalAlignment="Center" Margin="0,125,0,0" TextWrapping="Wrap" Text="" TextAlignment="Center" VerticalAlignment="Top" Width="509"/>
        <TextBlock x:Name="txtCorrect" HorizontalAlignment="Center" Margin="0,141,0,0" TextWrapping="Wrap" Text="" TextAlignment="Center" VerticalAlignment="Top" Width="508"/>
        <TextBlock x:Name="txtIncorrect" HorizontalAlignment="Center" Margin="0,157,0,0" TextWrapping="Wrap" Text="" TextAlignment="Center" VerticalAlignment="Top" Width="508"/>
        <TextBlock x:Name="txtcorrectPercentage" HorizontalAlignment="Center" Margin="0,173,0,0" TextWrapping="Wrap" Text="" TextAlignment="Center" VerticalAlignment="Top" Width="509"/>
        <Button Content="Back" HorizontalAlignment="Center" Margin="0,348,0,0" VerticalAlignment="Top" Width="64" Height="23" Click="backBtnClicked"/>
        <Button Content="Logout" HorizontalAlignment="Center" Margin="0,385,0,0" VerticalAlignment="Top" Click="logOutBtnClicked"/>


    </Grid>
</Window>
